RewriteEngine On
RewriteRule  plugins/(.*)                    plugins/$1                     [L]
RewriteRule  css/(.*)                        css/$1                         [L]
RewriteRule  js/(.*)                         js/$1                          [L]
RewriteRule signin_cntr$                    controller/signin_cntr.php      [L]
RewriteRule users_cntr$                      controller/users_cntr.php      [L]
RewriteRule feedback_cntr/$                  controller/feedback_cntr.php   [L]
RewriteRule message_cntr$                    controller/message_cntr.php    [L]
RewriteRule messagegroup_cntr$               controller/messagegroup_cntr.php    [L]
RewriteRule lostitem_cntr-(\w+)-(\w+)/$      controller/lostitem_cntr.php?action=$1&itemid=$2   [L]
RewriteRule schedule_cntr$                   controller/schedule_cntr.php   [L]
RewriteRule messagedetails_cntr$             controller/messagedetails_cntr.php [L]
RewriteRule myacitivity_cntr$                       controller/myacitivity_cntr.php     [L]
RewriteRule myacitivity_cntr-(\w+)-(\w+)$           controller/myacitivity_cntr.php?action=$1&id=$2     [L]
RewriteRule myacitivity_cntr-(\w+)-(\w+)-(\w+)$     controller/myacitivity_cntr.php?action=$1&id=$2&option=$3     [L]
RewriteRule survey_cntr$                     controller/survey_cntr.php     [L]
RewriteRule survey_cntr-(\w+)-(\w+)$         controller/survey_cntr.php?action=$1&id=$2     [L]
RewriteRule survey_cntr-(\w+)-(\w+)-(\w+)$   controller/survey_cntr.php?action=$1&id=$2&option=$3     [L]
RewriteRule timehall_cntr$                   controller/timehall_cntr.php     [L]
RewriteRule timehall_cntr-(\w+)-(\w+)$       controller/timehall_cntr.php?action=$1&id=$2     [L]
RewriteRule timehallchat_cntr$               controller/timehallchat_cntr.php     [L]
RewriteRule photo_cntr$                   controller/photo_cntr.php     [L]
RewriteRule photo_cntr-(\w+)-(\w+)$       controller/photo_cntr.php?action=$1&id=$2     [L]
RewriteRule friend_cntr$                   controller/friend_cntr.php     [L]
RewriteRule friend_cntr-(\w+)-(\w+)$       controller/friend_cntr.php?action=$1&id=$2     [L]
RewriteRule video_cntr$                   controller/video_cntr.php     [L]
RewriteRule video_cntr-(\w+)-(\w+)$       controller/video_cntr.php?action=$1&id=$2     [L]
RewriteRule activitie_cntr$                   controller/activitie_cntr.php     [L]
RewriteRule activitie_cntr-(\w+)-(\w+)$       controller/activitie_cntr.php?action=$1&id=$2     [L]
RewriteRule ^$                               view/                          [L]
RewriteRule home$                            view/login.php                 [L]
RewriteRule login$                           view/login.php                 [L]
RewriteRule friends$                           view/friends.php                 [L]
RewriteRule signup$                          view/signup.php                [L]
RewriteRule profile$                         view/profile.php               [L]
RewriteRule signout$                         view/signout.php               [L]
RewriteRule dashboard$                       view/dashboard.php             [L]
RewriteRule feedback$                        view/feedback.php              [L]
RewriteRule signout$                         view/signout.php               [L]
RewriteRule ^messages$                           view/messages.php                  [L]
RewriteRule groupchat$                       view/groupchat.php             [L]
RewriteRule myacitivity$                            view/myacitivity.php                                [L]
RewriteRule survey$                          view/survey.php                                [L]
RewriteRule timehall$                        view/timehall.php                              [L]
RewriteRule timehall-chat$                   view/timehall-chat.php                         [L]
RewriteRule timehall-chat-(\w+)$             view/timehall-chat.php?action=$1               [L]
RewriteRule timehall-chat-(\w+)-(\w+)$       view/timehall-chat.php?action=$1&id=$2         [L]
RewriteRule posts-(\w+)$                     view/posts.php?action=$1                       [L]
RewriteRule posts-(\w+)-(\w+)$               view/posts.php?action=$1&id=$2                [L]
RewriteRule search-([^/]+)-([^/]+)$              view/search.php?action=$1&id=$2               [L]